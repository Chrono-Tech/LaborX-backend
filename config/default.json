{
  "keystone": {
    "cookieSecret": "place-your-cookie-secret-here"
  },
  "storage": {
    "files": "/home/files",
    "url": "mongodb://localhost:27017/profile-backend"
  },
  "cloudinary": {
    "url": "place-your-cloudinary-url-here"
  },
  "self": {
    "url": "http://localhost:3001"
  },
  "rabbit": {
    "url": "amqp://guest:guest@localhost:5672",
    "reconnectTimeout" : 1000
  },
  "frontend": {
    "url": "https://exchange.tp.ntr1x.com"
  },
  "mail": {
    "from": "place-your-from@address.here",
    "baseURL": "http://localhost:8080",
    "transport": {
      "host": "smtp.mail.ru",
      "port": 465,
      "secure": true,
      "auth": {
        "user": "place-your-from@address.here",
        "pass": "place-your-password-here"
      }
    }
  },
  "sms": {
    "accountSid": "place-account-sid-here",
    "authToken": "palce-auth-token-here",
    "from": "place-from-phone-number-here"
  },
  "example-log": {
    "file": {
      "type": "rotating-file",
      "path": "/var/log/laborx.profile.backend/backend.log",
      "period": "1d",
      "count": 3
    },
    "options": {
      "src": false
    },
    "level": "info"
  }
}
